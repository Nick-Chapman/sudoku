
executable-name:
  echo sudoku.exe > executable-name

include ../../jenga/rulelib/haskell.jenga

main-entry:
  echo Sudoku > main-entry

ghc-version:
  echo 9.8.4 > ghc-version

ghc-options:
  touch ghc-options
  echo -Werror >>ghc-options
  echo -Wall >>ghc-options
  echo -Wno-name-shadowing >>ghc-options
  echo -O2 >>ghc-options

default-extensions:
  touch default-extensions
  echo LambdaCase >>default-extensions
